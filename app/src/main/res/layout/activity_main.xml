        android:layout_width="122dp"
        android:layout_height="30dp"
        android:layout_marginTop="74dp"
        android:visibility="invisible"

        app:layout_constraintBottom_toTopOf="@+id/light_btn2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/light_off"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/light_off"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="224dp"
        android:background="@color/colorAccent"
        android:text="@string/light_1_string"
        android:textColor="@color/avatar_icon_image_background"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246" />

    <Button
        android:id="@+id/light_btn"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="224dp"
        android:background="#000000"
        android:text="@string/light_1_string"
        android:textColor="@color/colorAccent"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246" />

    <Button
        android:id="@+id/light_btn1"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginTop="224dp"
        android:layout_marginEnd="50dp"
        android:background="@color/colorPrimaryDark"
        android:text="@string/light_2_string"
        android:textColor="@color/colorAccent"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toEndOf="@+id/light_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246" />

    <Button
        android:id="@+id/light_btn2"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginStart="50dp"
        android:background="@color/colorPrimaryDark"
        android:text="@string/light_3_string"
        android:textColor="@color/colorAccent"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
