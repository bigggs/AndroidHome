        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/light_btn1"
        app:layout_constraintVertical_bias="0.402" />

    <Button
        android:id="@+id/light_off2"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginStart="50dp"
        android:background="@color/colorAccent"
        android:text="@string/light_3_string"
        android:textColor="@color/avatar_icon_image_background"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/light_btn3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/light_btn"
        app:layout_constraintVertical_bias="0.402" />

    <Button
        android:id="@+id/light_off3"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_marginEnd="50dp"
        android:background="@color/colorAccent"
        android:text="@string/light_4_string"
        android:textColor="@color/avatar_icon_image_background"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/light_btn1"
        app:layout_constraintVertical_bias="0.394" />

    <SeekBar
        android:id="@+id/lux1"
        android:layout_width="122dp"
        android:layout_height="30dp"
        android:layout_marginTop="194dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/light_off"
        app:layout_constraintEnd_toStartOf="@+id/lux2"
        app:layout_constraintHorizontal_bias="0.48"
